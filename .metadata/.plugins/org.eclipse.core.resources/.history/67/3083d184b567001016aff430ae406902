import java.util.Scanner;

public class Matrix {
    public static void main(String[] args) {
        int[][] a = new int[2][2];
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter values for 3x3 matrix:\n");
        
        for (int i = 0; i < 2; i++) {
        for (int j = 0; j < 2; j++) {
            a[i][j] = sc.nextInt();
            }
        }

        System.out.println("The 3x3 Matrix is:");
        for (int[] row : a) {
        for (int value : row) {
         System.out.print(value + " ");
            }
            System.out.println();
        }
        sc.close();
    }
}
